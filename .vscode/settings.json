    // "java": "cd $dir && mkdir -p .Trash && javac -g *.java -d .Trash ${file} && java -cp .Trash ${fileBasenameNoExtension} && rm -r -f .Trash",
    // "java": "cd $dir && mkdir -p .Trash && javac -g -d .Trash $fileName && java -cp .Trash $fileNameWithoutExt && rm -rf .Trash",

    // "java": "cd $dir && mkdir -p .Trash && javac -g -d .Trash $fileName && java -cp .Trash $fileNameWithoutExt",
    // "java": "cd $dir && echo 'Current directory: $dir' && mkdir -p .Trash && echo 'Created .Trash directory' && javac -g -d .Trash $fileName && echo 'Compiled Java file to .Trash' && java -cp .Trash $fileNameWithoutExt && echo 'Ran Java program' && rm -rf .Trash && echo 'Deleted .Trash directory'",
    // "java": "cd $dir && echo 'Current directory: $dir' && mkdir -p .Trash && echo 'Created .Trash directory' && javac -g -d .Trash $fileName && echo 'Compiled Java file to .Trash' && ls .Trash && java -cp .Trash $fileNameWithoutExt && echo 'Ran Java program'",

    // "java": "cd $dir && mkdir -p .Trash && clear && javac -g -d .Trash $fileName &&  echo && rm -rf .Trash",
    "java": "cd $dir && mkdir -p .Trash && clear && javac -g -d .Trash $fileName && java -cp .Trash $fileNameWithoutExt && echo && rm -rf .Trash",

    // "java": "cd $dir && mkdir -p .Trash && echo '----------------------------------------' && javac -g -d .Trash $fileName && echo && java -cp .Trash $fileNameWithoutExt && echo && rm -rf .Trash && echo",

    // "java": "cd $dir && @mkdir -p .Trash && echo && @javac -g -d .Trash $fileName && echo && @java -cp .Trash $fileNameWithoutExt && echo && @rm -rf .Trash && echo",

    // "java": "cd $dir && (mkdir -p .Trash > /dev/null 2>&1 && echo && javac -g -d .Trash $fileName > /dev/null 2>&1 && echo && java -cp .Trash $fileNameWithoutExt > /dev/null 2>&1 && echo && rm -rf .Trash > /dev/null 2>&1 && echo)",

    "python": "cd $dir && python $fileName",
    "php": "cd $dir && php $fileName",
    "perl": "cd $dir && perl $fileName",
    "ruby": "cd $dir && ruby $fileName",
    "go": "cd $dir && go run $fileName",
    "lua": "cd $dir && lua $fileName",
    "groovy": "cd $dir && groovy $fileName",
    "powershell": "cd $dir && powershell -File $fileName",
    "bat": "cd $dir && $fileName",
    "shell": "cd $dir && bash $fileName",
    "fsharp": "cd $dir && fsharpc $fileName && mono $fileNameWithoutExt.exe",
    "csharp": "cd $dir && csc $fileName && $dir$fileNameWithoutExt",
    "haskell": "cd $dir && runhaskell $fileName",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "r": "cd $dir && Rscript $fileName",
    "dart": "cd $dir && dart $fileName",
    "julia": "cd $dir && julia $fileName",
    "kotlin": "cd $dir && kotlinc $fileName -include-runtime -d $fileNameWithoutExt.jar && java -jar $fileNameWithoutExt.jar",
    "scala": "cd $dir && scalac $fileName && scala $fileNameWithoutExt",
    "swift": "cd $dir && swift $fileName",
    "vb": "cd $dir && vbc $fileName && $dir$fileNameWithoutExt",
    "typescript": "cd $dir && tsc $fileName && node $fileNameWithoutExt",
    "javascript": "cd $dir && node $fileName",
    "coffeescript": "cd $dir && coffee $fileName",
    "json": "cd $dir && jq $fileName"
  },
