    "**/.*",
    "**/.*/**",
    "**/.vscode",
    "**/.vscode/**"
  ],
  "C_Cpp_Runner.useAddressSanitizer": true,
  "C_Cpp_Runner.useUndefinedSanitizer": true,
  "C_Cpp_Runner.useLeakSanitizer": true,
  "C_Cpp_Runner.showCompilationTime": true,
  "C_Cpp_Runner.useLinkTimeOptimization": true,
  "C_Cpp_Runner.msvcSecureNoWarnings": true,
  "code-runner.executorMap": {
    //"cpp": "cd $dir && clang++ -std=c++2b -g $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "c": "cd $dir && clang -g $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "cpp": "cd $dir && mkdir -p .Trash && clang++ -std=c++2b -g *.cpp -o .Trash/$fileNameWithoutExt && .Trash/$fileNameWithoutExt && rm -r -f .Trash",
    // "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    // "java": "cd $dir && javac *.java && java $fileNameWithoutExt",
